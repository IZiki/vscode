	"jstp": {
		"prefix": "jstp",
		"body": [
			"<script type=\"text/template\" name=\"$1\">"
			"$2"
			"</script>"
		],
		"description": "javascript template"
	},
	"nt-li": {
		"prefix": "nt-li",
		"body": [
			"<li>"
			"	$1<a href=\"\">$2</a>"
			"	<span>"
			"		$3"	
			"	</span>"
			"</li>"
		],
		"description": "notebook-li"
	},
	"nt-block": {
		"prefix": "nt-b",
		"body": [
			"<div class=\"u-block\">"
			"	<h3>Canvas<span>-</span></h3>"
			"	<div class=\"u-list\">"
			"		<h4>[<span>-</span>] 核心</h4>"
			"		<ul>"
			"			$1"
			"		</ul>"
			"	</div>"
			"</div>"
		],
		"description": "notebook block"
	},
	"nt-table": {
		"prefix": "nt-table",
		"body": [
			"<table>"
			"	<tr>"
			"		<th></th>"
