	"silky-repeat": {
		"prefix": "silky-repeat",
		"body": [
			"{{#repeat $1}}"
				"{{\$index}}"
			"{{/repeat}}"
		],
		"description": "silky-repeat"
	},
	"hbs-each": {
		"prefix": "each",
		"body": [
			"{{#each this}}"
