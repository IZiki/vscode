{
	"loadcss": {
		"prefix": "loadcss",
		"body": [
			"<link rel=\"stylesheet\" href=\"../$1.css\" />"
		],
		"description": "link css"
	},
	"loadjs": {
		"prefix": "loadjs",
		"body": [
			"<script src=\"$1\"></script>"
		],
		"description": "link css"
	},
	"htmldemo": {
