{
	"JSFunction": {
		"prefix": "fn",
		"body": [
			"function $1($2){"
			"	$3"
			"}"
		],
		"description": "Javascript Function"
	},
	"JSArrowFunction3": {
		"prefix": "fnn",
		"body": [
			"let $1 = ($2) => {$3}"
		],
		"description": "Javascript Arrow Function"
	},
	"JSConsole": {
		"prefix": "cs",
		"body": [
			"console.log($1)"
		],
		"description": "Javascript Console.log"
	},
	"JSConsoleE": {
		"prefix": "ce",
		"body": [
			"console.error($1)"
		],
		"description": "Javascript Console.log"
	},
	"NDrequire": {
		"prefix": "rq",
		"body": [
			"require('$1');"
		],
		"description": "Node require modules"
	},
	"ESimport": {
		"prefix": "im",
		"body": [
			"import $1 from '$2'"
		],
		"description": "ES import modules"
	},
	"NDexports": {
		"prefix": "exs",
		"body": [
			"module.exports = $1"
		],
		"description": "Node module exports"
	},
	"ESxport": {
		"prefix": "ex",
		"body": [
			"export default $1"
		],
		"description": "ES module export"
	},
	"JQclear": {
		"prefix": "jqclear",
		"body": [
			"jQuery.noConflict();"
		],
		"description": ""
	},
	"JQready": {
		"prefix": "jqready",
		"body": [
			"jQuery(function($){"
			"	$1"
			"});"
		],
		"description": ""
	},
	"JQselect": {
		"prefix": "$",
		"body": [
			"$('$1')$2"
		],
		"description": ""
	},
	"JQajax": {
		"prefix": "jqajax",
		"body": [
			"$.ajax({"
			"	async:true,"
			"	url:$1,"
			"	type:'GET',"
			"	data:{'$2':$3},"
			"	dataType:'jsonp',"
			"	success:function (data){"
			"		if(data.result!=0){"
			"			console.error('接口数据异常',data.msg)"
			"			return"
			"		}"
			"	},"
			"	error:function (){"
			"		console.error('接口连接异常')"
			"	}"
			"})"
		],
		"description": ""
	},
	"JQplugin": {
		"prefix": "jqplugin",
		"body": [
			"$.fn.extend({"
			"	'foo':function(arg){"
			"		$1"
			"		return this;	//this即为操作的jq对象"	
			"	}"
			"})"
		],
		"description": ""
	},
}